<h2>Items</h2>

<table>
<thead>
  <tr>
    <th>Item</th>
    <th>Price</th>
    <th>Location</th>
  </tr>
</thead>
<tbody>
<% @list.each do |data| %>
<tr>
  <td><%= data[:item] %></td>
  <td><%= data[:price] %></td>
  <td><%= data[:location] %></td>
</tr>
<% end %>
</tbody>
</table>
<button><a href='/new'>Add!</a></button>
<button><a href='/deals'>Deals!</a></button>